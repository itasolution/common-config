gzip on;
gzip_types text/plain text/html text/xml application/xml application/xhtml+xml text/javascript text/css application/x-javascript;

# A better example
gzip on;
gzip_min_length  1000;
gzip_buffers  8 32k;
gzip_proxied     expired no-cache no-store private auth;
gzip_types    text/plain application/x-javascript text/xml text/css application/xml application/javascript image/jpeg image/png image/gif ;
gzip_vary on;
gzip_comp_level 4;
gzip_static on;